<%= link_to 'ログアウト', destroy_user_session_path, method: :delete %>
<%= link_to 'ユーザー一覧', users_path %>
<%= link_to 'プロフィール', users_my_profile_path %>
<h1>ユーザー一覧</h1>

<% @user.each do |user| %>
  <ul class="user-list">
    <a href="/users/<%= user.id %>" class="user-link">
      <li class="user-item">
        <%= user.email %>
      </li>
    </a>
  </ul>
<% end %>

<%= paginate @user %>